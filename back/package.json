    "e:test": "dotenv -e .env.test.local -e .env.test -e .env.local -e .env"
  },
  "dependencies": {
    "apollo-server-express": "^2.8.1",
    "express": "^4.17.1",
    "graphql": "^14.4.2",
    "socket.io": "^2.2.0"
  },
