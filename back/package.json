  "scripts": {
    "dev": "yarn e:dev nodemon src/index.js",
    "docker:up": "yarn e:dev docker-compose up",
    "docker:reset": "docker-compose down && docker-compose rm -f",
    "docker:stop:all": "docker stop $(docker ps -aq)",
